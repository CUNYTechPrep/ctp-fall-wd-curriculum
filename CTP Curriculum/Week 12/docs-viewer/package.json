{
  "name": "week12-docs-viewer",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "extract-types": "node scripts/extract-types.js",
    "bundle-sources": "node scripts/generate-source-bundle.js",
    "prepare-monaco": "npm run extract-types && npm run bundle-sources",
    "prebuild": "npm run prepare-monaco",
    "predev": "npm run prepare-monaco",
    "prestart": "npm run prepare-monaco",
